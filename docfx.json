{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "docs",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "toc.yml", "index.md" ]
      },
      {
        "files": [ "docs/**.yml", "docs/index.md" ],
        "src": ".",
        "dest": "api"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "dest": "docs/_site",
    "globalMetadata": {
      "_appTitle": "CSE325 Team Project Docs",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "template": [
      "default",
      "templates/custom"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}